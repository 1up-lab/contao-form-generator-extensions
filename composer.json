{
  "name":"oneup/contao-form-generator-extensions",
  "description":"This Contao extension extends the core form generator with some useful stuff.",
  "keywords":["contao", "form generator", "extensions"],
  "type":"contao-module",
  "homepage":"http://1up.io",
  "license":"MIT",
  "authors":[
    {
      "name":"David Greminger",
      "email":"dg@1up.io",
      "homepage":"http://1up.io",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"hello@1up.io",
    "issues":"https://github.com/1up-lab/contao-form-generator-extensions/issues",
    "irc":"irc://irc.freenode.org/contao.dev",
    "source":"https://github.com/1up-lab/contao-form-generator-extensions"
  },
  "require":{
    "php":">=5.4",
    "contao/core":">=3.2,<4.0-dev",
    "contao-community-alliance/composer-plugin": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Oneup\\Contao\\": "src"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/form-generator-extensions"
      }
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
